{
  "name": "@upik/directus-extension-codeup",
  "version": "1.0.0",
  "directus:extension": {
    "host": "^10.6.3",
    "type": "bundle",
    "path": {
      "app": "dist/app.js",
      "api": "dist/api.js"
    },
    "entries": [
      {
        "type": "endpoint",
        "name": "endpoints",
        "source": "src/endpoints/index.ts"
      }
    ]
  },
  "description": "Codeup backend app extensions bundle",
  "icon": "extension",
  "keywords": [
    "directus",
    "directus-extension",
    "directus-custom-bundle"
  ],
  "files": [
    "dist",
    "!**/*.test.{d.ts,js}",
    "!**/*.d.ts.map"
  ],
  "scripts": {
    "build": "directus-extension build",
    "dev": "directus-extension build -w --no-minify",
    "link": "directus-extension link",
    "add": "directus-extension add"
  },
  "devDependencies": {
    "@directus/extensions-sdk": "10.1.11",
    "@types/node": "^20.8.5",
    "typescript": "^5.2.2",
    "vue": "^3.3.4"
  }
}
